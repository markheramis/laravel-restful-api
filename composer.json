{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.1",
        "authy/php": "^4.0.0-rc2",
        "bacon/bacon-qr-code": "^2.0",
        "brianium/paratest": "^6.6",
        "cartalyst/sentinel": "^6.0",
        "corbosman/laravel-passport-claims": "^4.0",
        "doctrine/dbal": "^3.4",
        "fruitcake/laravel-cors": "^3.0",
        "gamegos/php-code-sniffer": "^0.6.1",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^9.3",
        "laravel/passport": "^11.0",
        "laravel/tinker": "^2.7",
        "pragmarx/google2fa-laravel": "^2.0",
        "pragmarx/google2fa-qrcode": "^3.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^7.0",
        "ramsey/uuid": "^4.1",
        "sentry/sentry-laravel": "^2.12",
        "spatie/laravel-activitylog": "^4.3",
        "spatie/laravel-fractal": "^6.0",
        "spatie/laravel-sluggable": "^3.1",
        "twilio/sdk": "^6.28"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.7",
        "fakerphp/faker": "^1.9.1",
        "knuckleswtf/scribe": "^4.16",
        "laravel/dusk": "^6.2",
        "laravel/envoy": "^2.6",
        "laravel/sail": "^1.0.1",
        "laravel/telescope": "^4.8",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "overtrue/phplint": "^4.0",
        "phpbench/phpbench": "1.2.x-dev",
        "phpunit/php-code-coverage": "^9.2",
        "phpunit/phpunit": "^9.5.10"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/factories",
            "database/seeders"
        ],
        "files": [
            "app/Http/Helper/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Tests\\Benchmark\\": "tests/Benchmark/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "sail": [
            "./vendor/bin/sail"
        ],
        "doc": [
            "@php artisan scribe:generate --force"
        ],
        "lint": [
            "./vendor/bin/phplint"
        ],
        "bench": [
            "@php vendor/bin/phpbench run"
        ]
    }
}
